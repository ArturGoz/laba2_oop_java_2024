<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="Gun">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Model" type="xs:string" />
                <xs:element name="Handy" type="xs:string" />
                <xs:element name="Origin" type="xs:string" />
                <xs:element name="TTC">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Range" type="xs:string" />
                            <xs:element name="SightingRange" type="xs:unsignedShort" />
                            <xs:element name="Magazine" type="xs:boolean" />
                            <xs:element name="Optics" type="xs:boolean" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Material" type="xs:string" />
            </xs:sequence>
            <xs:attribute name="id" type="xs:string" use="required" />
        </xs:complexType>
    </xs:element>
</xs:schema>